<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MemeFeedActivity">

    <TextView
        android:id="@+id/memeFeedtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/memeFeedPage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/generalSpace"
        android:layout_marginTop="@dimen/generalSpace"
        android:src="@drawable/ic_baseline_arrow_back_24"
        app:tint="@color/fresco_pink"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="?android:borderlessButtonStyle" />




</androidx.constraintlayout.widget.ConstraintLayout>